var 
	request = require("request"),
	cheerio = require("cheerio");
	
	request('hhttp://www.tennisabstract.com/cgi-bin/leaders.cgi', function (error, response, body) {
		if (!error && response.statusCode == 200) {
			var tournament;
			var $ = cheerio.load(body);

			tournament = $().map(function(i,link) { return $(link).text();})

		}	
	})
